    "flow_increment": {
      "id": "flow_increment",
      "name": "Increment Counter",
      "trigger": {
        "type": "onClick",
        "componentId": "comp_increment"
      },
      "nodes": [
        {
          "id": "node_event_inc",
          "type": "event",
          "position": { "x": 100, "y": 150 },
          "config": {
            "eventType": "onClick",
            "componentId": "comp_increment"
          }
        },
        {
          "id": "node_setState_inc",
          "type": "setState",
          "position": { "x": 350, "y": 150 },
          "config": {
            "variable": "count",
            "operation": "increment",
            "value": 1
          }
        },
        {
          "id": "node_console_inc",
          "type": "console",
          "position": { "x": 600, "y": 150 },
          "config": {
            "level": "log",
            "message": "Counter incremented!"
          }
        }
      ],
      "edges": [
        { "id": "edge_inc_1", "source": "node_event_inc", "target": "node_setState_inc" },
        { "id": "edge_inc_2", "source": "node_setState_inc", "target": "node_console_inc" }
      ]
    },
